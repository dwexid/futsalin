<div class="title"><%= @content_title %></div>
<div class="row">
    <div class="col w-100">
        <table class="table table-hover table-bordered" style="background:#fff;">
            <thead>
                <tr>
                    <th>No</th>
                    <th>Owner</th>
                    <th>Nama tempat</th>
                    <th>Alamat</th>
                    <th>Kontak</th>
                </tr>
            </thead>
            <tbody>
            <% i=1 %>
            <% @owners.each do |own| %>
                <tr>
                    <td><%= i+=1 %></td>
                    <td><%= own.fullname %></td>
                    <td><%= own.nama_tempat %></td>
                    <td><%= own.alamat %></td>
                    <td><%= own.kontak %></td>
                </tr>
            <% end %>
            </tbody>    
        </table>
    </div>
</div>
</div>